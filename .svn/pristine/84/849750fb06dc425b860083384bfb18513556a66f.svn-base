<div class="row">
	<div class="col col-lg-12">
		<div class="panel panel-default">
			<div class="panel-heading">
				<h3 class="panel-title">
					产品管理
						<a class="text-info btn btn-default btn-xs pull-right" ng-click="accordion(true)">
							全部展开
						</a>
						<a class="text-muted btn btn-default btn-xs pull-right" ng-click="accordion(false)">
							全部折叠
						</a>
					
				</h3>
			</div>

			<div class="panel-body">
				<form name="editForm" class="">

					<div class="container-fluid">
						<div class="panel-group" id="accordion">
							<div class="panel panel-default">
								<div class="panel-heading">
									<h4 class="panel-title">
										<a class="accordion-toggle" data-toggle="collapse"
											data-parent="#accordion" href="#collapseBase">基本信息</a>
									</h4>
								</div>
								<div id="collapseBase" class="panel-collapse collapse in">
									<div class="panel-body">

										<div class="row"  ng-mouseenter="tips_sku=true;" ng-mouseleave="tips_sku=false;">
											<div class="col col-sm-12">
												<div class="form-group"
													ng-class="{'has-error': editForm.sku.$invalid}">
													<label class="control-label">SKU
														<span class="text-info" href="#" data-toggle="tooltip" data-delay="0" data-original-title="SKU:唯一，不能多于30位，不能用特殊字符（除去-，#）like:110-15232-51" ng-show="tips_sku">Tips</span>
													</label>
													<input type="text" name="sku" ng-model="item.sku"
														class="form-control" ng-pattern="/^[0-9a-zA-Z\#\-]{10,30}$/" required>
													<span ng-show="editForm.sku.$error.required"
														class="help-inline">Required</span>
												</div>
											</div>
										</div>

										<div class="row" ng-init="tips_description=false;"  ng-mouseenter="tips_description=true;" ng-mouseleave="tips_description=false;">
											<div class="col col-sm-12">
												<div class="form-group"
													ng-class="{'has-error': editForm.description.$invalid}">
													<label class="control-label">描述
													<span class="text-info" href="#" data-toggle="tooltip" title="产品的简要描述" ng-show="tips_description">Tips</span>													
													</label>
													<textarea name="description" class="form-control"
														ng-model="item.description" required></textarea>
													<span ng-show="editForm.description.$error.required"
														class="help-inline">Required</span>
												</div>
											</div>
										</div>

										<div class="row">
											<div class="col col-sm-4" ng-init="tips_category=false;" ng-mouseenter="tips_category=true;" ng-mouseleave="tips_category=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.category.$invalid}">
													<label class="control-label">产品类别
													<span class="text-info" href="#" data-toggle="tooltip" title="产品的分类" ng-show="tips_category">Tips</span>	
													</label>
													<select name="category" class="form-control"
														ng-model="item.category_id"
														ng-options="c.id as c.name for c in categories" required></select>
													<span ng-show="editForm.category.$error.required"
														class="help-inline">Required</span>
												</div>
											</div>
											<div class="col col-sm-4" ng-init="tips_status=false;" ng-mouseenter="tips_status=true;" ng-mouseleave="tips_status=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.status.$invalid}">
													<label class="control-label">产品状态
														<span class="text-info" href="#" data-toggle="tooltip" title="产品状态:新，比较新，正常，夕阳，停止" ng-show="tips_status">Tips</span>	
													</label>
													<select name="status" class="form-control"
														ng-init="statuses=[{data:'新'},{data:'比较新'},{data:'正常'},{data:'夕阳'},{data:'停止'}];"
														ng-model="item.status"
														ng-options="s.data as s.data for s in statuses" required></select>
												</div>
											</div>
											
											<div class="col col-sm-4" ng-init="tips_other=false;" ng-mouseenter="tips_other=true;" ng-mouseleave="tips_other=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.other.$invalid}">
													<label class="control-label">其他信息
														<span class="text-info" href="#" data-toggle="tooltip" title="其他必备信息,绿色为是,白色为否" ng-show="tips_other">Tips</span>
													</label>
													<div class="btn-group ">
														<button type="button" class="btn btn-default"
															ng-class="{'btn-success': item.other.dropship}"
															ng-model="item.other.dropship" btn-checkbox>Dropship</button>
														<button type="button" class="btn btn-default"
															ng-class="{'btn-success': item.other.active}"
															ng-model="item.other.active" btn-checkbox>Active</button>
														<button type="button" class="btn btn-default"
															ng-class="{'btn-success': item.other.virtual}"
															ng-model="item.other.virtual" btn-checkbox>Virtual</button>
														<button type="button" class="btn btn-default"
															ng-class="{'btn-success': item.other.inventory}"
															ng-model="item.other.inventory" btn-checkbox>Inventory</button>
													</div>
												</div>
											</div>
										</div>
										

									</div>
								</div>
							</div>
							<div class="panel panel-default">
								<div class="panel-heading">
									<h4 class="panel-title">
										<a class="accordion-toggle" data-toggle="collapse"
											data-parent="#accordion" href="#collapseShip">物流信息</a>
									</h4>
								</div>
								<div id="collapseShip" class="panel-collapse collapse">
									<div class="panel-body">
										<div class="row">
										
											<div class="col col-sm-3" ng-init="tips_length=false;" ng-mouseenter="tips_length=true;" ng-mouseleave="tips_length=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.length.$invalid}">
													<label class="control-label">
													产品-长(mm)
													<span class="text-info" href="#" data-toggle="tooltip" title="单位(mm) 正整数" ng-show="tips_length">Tips</span>													
													</label>
													<input type="text" ng-pattern="/^[0-9]+$/"
														name="length" class="form-control" ng-model="item.length"
														required>
												</div>
											</div>
											
											<div class="col col-sm-3" ng-init="tips_width=false;" ng-mouseenter="tips_width=true;" ng-mouseleave="tips_width=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.width.$invalid}">
													<label class="control-label">产品-宽(mm)
													<span class="text-info" href="#" data-toggle="tooltip" title="单位(mm) 正整数" ng-show="tips_width">Tips</span>	
													</label>													
													<input type="text" ng-pattern="/^[0-9]+$/"
														name="width" class="form-control" ng-model="item.width"
														required>
												</div>
											</div>
											
											<div class="col col-sm-3" ng-init="tips_height=false;" ng-mouseenter="tips_height=true;" ng-mouseleave="tips_height=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.height.$invalid}">
													<label class="control-label">产品-高(mm)
													<span class="text-info" href="#" data-toggle="tooltip" title="单位(mm) 正整数" ng-show="tips_height">Tips</span>	
													</label>
													<input type="text" ng-pattern="/^[0-9]+$/"
														name="height" class="form-control" ng-model="item.height"
														required>
												</div>
											</div>
											
											<div class="col col-sm-3" ng-init="tips_weight=false;" ng-mouseenter="tips_weight=true;" ng-mouseleave="tips_weight=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.weight.$invalid}">
													<label class="control-label">产品-重量(g)
													<span class="text-info" href="#" data-toggle="tooltip" title="单位(g) 正整数" ng-show="tips_weight">Tips</span>	
													</label>
													<input type="text" ng-pattern="/^[0-9]+$/"
														name="weight" class="form-control" ng-model="item.weight"
														required>
												</div>
											</div>
										</div>

										<div class="row">
											<div class="col col-sm-3" ng-init="tips_length_p=false;" ng-mouseenter="tips_length_p=true;" ng-mouseleave="tips_length_p=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.length_package.$invalid}">
													<label class="control-label">包装-长(mm)
													<span class="text-info" href="#" data-toggle="tooltip" title="单位(mm) 正整数" ng-show="tips_length_p">Tips</span>	
													</label>
													<input type="text" ng-pattern="/^[0-9]+$/"
														name="length_package" class="form-control"
														ng-model="item.length_package" required>
												</div>
											</div>
											
											<div class="col col-sm-3" ng-init="tips_width_p=false;" ng-mouseenter="tips_width_p=true;" ng-mouseleave="tips_width_p=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.width_package.$invalid}">
													<label class="control-label">包装-宽(mm)
													<span class="text-info" href="#" data-toggle="tooltip" title="单位(mm) 正整数" ng-show="tips_width_p">Tips</span>	
													</label>
													<input type="text" ng-pattern="/^[0-9]+$/"
														name="width_package" class="form-control"
														ng-model="item.width_package" required>
												</div>
											</div>
											
											<div class="col col-sm-3" ng-init="tips_height_p=false;" ng-mouseenter="tips_height_p=true;" ng-mouseleave="tips_height_p=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.height_package.$invalid}">
													<label class="control-label">包装-高(mm)
													<span class="text-info" href="#" data-toggle="tooltip" title="单位(mm) 正整数" ng-show="tips_height_p">Tips</span>	
													</label>
													<input type="text" ng-pattern="/^[0-9]+$/"
														name="height_package" class="form-control"
														ng-model="item.height_package" required>
												</div>
											</div>
											
											<div class="col col-sm-3" ng-init="tips_weight_p=false;" ng-mouseenter="tips_weight_p=true;" ng-mouseleave="tips_weight_p=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.weight_package.$invalid}">
													<label class="control-label">包装-重量(g)
													<span class="text-info" href="#" data-toggle="tooltip" title="单位(g) 正整数" ng-show="tips_weight_p">Tips</span>	
													</label>
													<input type="text" ng-pattern="/^[0-9]+$/"
														name="weight_package" class="form-control"
														ng-model="item.weight_package" required>
												</div>
											</div>
											
										</div>

										<div class="row">
											<div class="col col-sm-3" ng-init="tips_length_c=false;"
												ng-mouseenter="tips_length_c=true;"
												ng-mouseleave="tips_length_c=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.length_carton.$invalid}">
													<label class="control-label">
														外箱-长(mm)
														<span class="text-info" href="#" data-toggle="tooltip"
															title="单位(cm) 正整数" ng-show="tips_length_c">Tips</span>
													</label>
													<input type="text" ng-pattern="/^[0-9]+$/"
														name="length_carton" class="form-control"
														ng-model="item.length_carton" required>
												</div>
											</div>

											<div class="col col-sm-3" ng-init="tips_width_c=false;"
												ng-mouseenter="tips_width_c=true;"
												ng-mouseleave="tips_width_c=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.width_carton.$invalid}">
													<label class="control-label">
														外箱-宽(mm)
														<span class="text-info" href="#" data-toggle="tooltip"
															title="单位(cm) 正整数" ng-show="tips_width_c">Tips</span>
													</label>
													<input type="text" ng-pattern="/^[0-9]+$/"
														name="width_carton" class="form-control"
														ng-model="item.width_carton" required>
												</div>
											</div>

											<div class="col col-sm-3" ng-init="tips_height_c=false;"
												ng-mouseenter="tips_height_c=true;"
												ng-mouseleave="tips_height_c=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.height_carton.$invalid}">
													<label class="control-label">
														外箱-高(mm)
														<span class="text-info" href="#" data-toggle="tooltip"
															title="单位(cm) 正整数" ng-show="tips_height_c">Tips</span>
													</label>
													<input type="text" ng-pattern="/^[0-9]+$/"
														name="height_carton" class="form-control"
														ng-model="item.height_carton" required>
												</div>
											</div>

											<div class="col col-sm-3" ng-init="tips_weight_c=false;"
												ng-mouseenter="tips_weight_c=true;"
												ng-mouseleave="tips_weight_c=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.weight_carton.$invalid}">
													<label class="control-label">
														整箱-重量(KG)
														<span class="text-info" href="#" data-toggle="tooltip"
															title="单位(KG) 正数" ng-show="tips_weight_c">Tips</span>
													</label>
													<input type="text" ng-pattern="/^[0-9]+\.?[0-9]*$/"
														name="weight_carton" class="form-control"
														ng-model="item.weight_carton" required>
												</div>
											</div>
										</div>
										
										<div class="row">
											<div class="col col-sm-3" ng-init="tips_spq=false;"
												ng-mouseenter="tips_spq=true;"
												ng-mouseleave="tips_spq=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.spq.$invalid}">
													<label class="control-label">
														装箱数(个)
														<span class="text-info" href="#" data-toggle="tooltip"
															title="单位(个) 正整数" ng-show="tips_spq">Tips</span>
													</label>
													<input type="text" ng-pattern="/^[0-9]+$/"
														name="spq" class="form-control"
														ng-model="item.spq" required>
												</div>
											</div>

											<div class="col col-sm-3" ng-init="tips_tray=false;"
												ng-mouseenter="tips_tray=true;"
												ng-mouseleave="tips_tray=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.tray.$invalid}">
													<label class="control-label">
														卡板数(个)
														<span class="text-info" href="#" data-toggle="tooltip"
															title="单位(个) 正整数" ng-show="tips_tray">Tips</span>
													</label>
													<input type="text" ng-pattern="/^[0-9]{0,9}$/"
														name="tray" class="form-control"
														ng-model="item.tray">
												</div>
											</div>

											<div class="col col-sm-3" ng-init="tips_density=false;"
												ng-mouseenter="tips_density=true;"
												ng-mouseleave="tips_density=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.density.$invalid}">
													<label class="control-label">
														单个体积重(kg)
														<span class="text-info" href="#" data-toggle="tooltip"
															title="请填上外箱长宽高,系统会自动计算! (正数)" ng-show="tips_density">Tips</span>
													</label>
													<div class="control-label" >{{(item.length_carton * item.width_carton * item.height_carton / 5000) || "请正确填写外箱长宽高"}}</div>
												</div>
											</div>
										</div>
										
										
										
									</div>
								</div>
							</div>
							<div class="panel panel-default">
								<div class="panel-heading">
									<h4 class="panel-title">
										<a class="accordion-toggle" data-toggle="collapse"
											data-parent="#accordion" href="#collapseCustom">海关信息</a>
									</h4>
								</div>
								<div id="collapseCustom" class="panel-collapse collapse">
									<div class="panel-body">
										<div class="row" ng-init="tips_hg_type=false;"
											ng-mouseenter="tips_hg_type=true;"
											ng-mouseleave="tips_hg_type=false;">

											<div class="col col-sm-12">
												<div class="form-group"
													ng-class="{'has-error': editForm.hg_type.$invalid}">
													<label class="control-label">
														海关型号
														<span class="text-info" href="#" data-toggle="tooltip"
															title="海关型号" ng-show="tips_hg_type">Tips</span>
													</label>
													<input type="text" name="hg_type" ng-model="item.hg_type"
														class="form-control">

												</div>
											</div>
										</div>

										<div class="row" ng-init="tips_description_hg_cn=false;"  ng-mouseenter="tips_description_hg_cn=true;" ng-mouseleave="tips_description_hg_cn=false;">
											<div class="col col-sm-12">
												<div class="form-group"
													ng-class="{'has-error': editForm.description_hg_cn.$invalid}">
													<label class="control-label">中文海关描述
													<span class="text-info" href="#" data-toggle="tooltip" title="中文海关描述" ng-show="tips_description_hg_cn">Tips</span>													
													</label>
													<textarea name="description_hg_cn" class="form-control"
														ng-model="item.description_hg_cn" required></textarea>
													<span ng-show="editForm.description_hg_cn.$error.required"
														class="help-inline">Required</span>
												</div>
											</div>
										</div>
										
										<div class="row" ng-init="tips_description_hg_en=false;"  ng-mouseenter="tips_description_hg_en=true;" ng-mouseleave="tips_description_hg_en=false;">
											<div class="col col-sm-12">
												<div class="form-group"
													ng-class="{'has-error': editForm.description_hg_en.$invalid}">
													<label class="control-label">英文海关描述
													<span class="text-info" href="#" data-toggle="tooltip" title="英文海关描述" ng-show="tips_description_hg_en">Tips</span>													
													</label>
													<textarea name="description_hg_en" class="form-control"
														ng-model="item.description_hg_en" required></textarea>
													<span ng-show="editForm.description_hg_en.$error.required"
														class="help-inline">Required</span>
												</div>
											</div>
										</div>
										
										<div class="row">
											<div class="col col-sm-6" ng-init="tips_reback_cn=false;"
												ng-mouseenter="tips_reback_cn=true;"
												ng-mouseleave="tips_reback_cn=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.reback_cn.$invalid}">
													<label class="control-label">
														中国退税率
														<span class="text-info" href="#" data-toggle="tooltip"
															title="单位(个) 正整数" ng-show="tips_reback_cn">Tips</span>
													</label>
													<input type="text" ng-pattern="/^[0-9]*\.?[0-9]+$/"
														name="reback_cn" class="form-control"
														ng-model="item.reback_cn" required>
												</div>
											</div>
											
											<div class="col col-sm-6" ng-init="tips_custom_code_cn=false;"
												ng-mouseenter="tips_custom_code_cn=true;"
												ng-mouseleave="tips_custom_code_cn=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.custom_code_cn.$invalid}">
													<label class="control-label">
														中国海关编码
														<span class="text-info" href="#" data-toggle="tooltip"
															title="中国海关编码" ng-show="tips_custom_code_cn">Tips</span>
													</label>
													<input type="text" name="custom_code_cn" class="form-control"
														ng-model="item.custom_code_cn">
												</div>
											</div>
						
											<div class="col col-sm-6" ng-init="tips_tariff_us=false;"
												ng-mouseenter="tips_tariff_us=true;"
												ng-mouseleave="tips_tariff_us=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.tariff_us.$invalid}">
													<label class="control-label">
														美国关税率
														<span class="text-info" href="#" data-toggle="tooltip"
															title="美国关税率" ng-show="tips_tariff_us">Tips</span>
													</label>
													<input type="text" ng-pattern="/^[0-9]*\.?[0-9]+$/"
														name="tariff_us" class="form-control"
														ng-model="item.tariff_us" required>
												</div>
											</div>
											
											<div class="col col-sm-6" ng-init="tips_custom_code_us=false;"
												ng-mouseenter="tips_custom_code_us=true;"
												ng-mouseleave="tips_custom_code_us=false;">
												<div class="form-group"
													ng-class="{'has-error': editForm.custom_code_us.$invalid}">
													<label class="control-label">
														美国海关编码
														<span class="text-info" href="#" data-toggle="tooltip"
															title="美国海关编码" ng-show="tips_custom_code_us">Tips</span>
													</label>
													<input type="text"
