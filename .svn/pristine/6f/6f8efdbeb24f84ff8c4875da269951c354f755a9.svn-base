<!--startprint-->
<style>

div.purchase-io{
   	margin-top:20px;
	margin-left:auto;
	margin-right:auto;
	padding-bottom:20px;
	

}
p.print_title{
	text-align:center;
	font-size:22px;
	font-style: bolder;
}
.table-total{
	border-collapse:collapse;
	width:100%;
	height:auto;
	font:"Times New Roman", Times, serif;
}
table.table-total tr td{
	border:1px solid #000000;
	text-align: center;
}
.table-total .tr0 td{
	font-size:15px;font-weight:normal;
}

.table-total .tr1 td,.table-total .tr1 th{
	border:1px solid black;
	height:25px;
	font-size:14px;font-weight:bold;
	text-align: center;
}

.table-total .tr2 td{
	border:1px solid black;
	height:25px;
	font-size:12px;font-weight:normal;
	text-align:center;
}
</style>
</head>
<body>
<div class="purchase-io">
<p class="print_title">深圳千岸进出口有限公司</p>

<table style="width:100%">
<tr><td><label>供应商:</label>{{print.vendor_id|vendor}}</td>
	<td><span><label>入库日期: </label>{{print.created_at}}</span></td>
	<td><span><label>入库编号:</label> {{print.relation_invoice}}</span> </td>
	<td><span><label>采购编号: </label>{{print.invoice}}</span></td>
</tr>
</table>
<table class="table-total">
<tr class="tr1"><td>sku</td><td>规格描述</td><td>数量</td></tr>
 <tr ng-repeat="sku in print.details ">
<td>{{sku.sku}}</td>
<td>{{sku.item.description}}
</td>
<td>{{sku.qty}}</td>
</tr>
<tr><td colspan="2"><strong>合计</strong></td><td><strong>{{print.qty}}</strong></td></tr>
</table>
</div>
<!--endprint-->
<OBJECT id=WebBrowser classid=CLSID:8856F961-340A-11D0-A96B-00C04FD705A2 height=0 width=0></OBJECT>
<p style="text-align: right">
<button   class="btn btn-primary" type="button" ng-click="printOrder('1')">
		打印
	</button>
	<button   class="btn btn-primary" type="button"  ng-click="printPreview()">
		打印预览
	</button>
</p>