<div class="row">
	<div class="col col-xs-6">
		<div class="panel panel-warning">
			<div class="panel-heading">
				<h3 class="panel-title"><span class="glyphicon glyphicon-th"></span> 货位管理</h3>
			</div>
			<div class="panel-body">

				<table class="table table-hover">
					<thead>
						<tr>
							<th>仓库名称</th>
							<th>货位列表</th>
						</tr>
					</thead>
					<tbody>
						<tr ng-repeat="wh in whList">
							<td>{{wh.name}}</td>
							<td>
								<a href="#/storage/{{wh.id}}" type="button" class="btn btn-xs btn-default"><span class="glyphicon glyphicon-list"></span> 货位列表</a>
								<button type="button" class="btn btn-xs btn-primary"><span class="glyphicon glyphicon-plus"></span> 新增货位</button>
							</td>
						</tr>
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>